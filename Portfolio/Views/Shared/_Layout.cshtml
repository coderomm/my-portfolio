<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewBag.Title</title>
    <meta charset="UTF-8">
    <meta name="description" content="Coder Om">
    <meta name="keywords" content="personal, portfolio">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <script src="https://cdn.ckeditor.com/ckeditor5/40.0.0/classic/ckeditor.js"></script>
    <!--favicon-->
    <link rel="icon" href="~/Content/projectAssets/Images/About/adminlogo.jpg" type="image/png">

    <!-- Stylesheets -->
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/cubeportfolio/css/cubeportfolio.min.css">
    <link rel="stylesheet" href="~/Content/css/owl.theme.css">
    <link rel="stylesheet" href="~/Content/css/owl.carousel.css">
    <link rel="stylesheet" href="~/Content/css/colors/red.css">
    <link rel="stylesheet" href="~/Content/css/style.css">
    <link href="~/Content/css/animated-profile.css" rel="stylesheet" />

    <!-- Fontawesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css">

    <!-- Google Web fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@500&family=Space+Grotesk:wght@400;500&display=swap&family=Share+Tech+Mono:wght@500&display=swap" rel="stylesheet">

    <!-- Font icons -->
    <link rel="stylesheet" href="~/Content/icon-fonts/fontawesome-5.0.6/css/fontawesome-all.min.css">
    <link rel="stylesheet" href="~/Content/icon-fonts/flaticon/flaticon.css">

</head>
<body>
    <!-- PRELOADER -->
    <div class="preloader">
        <div class="loader">
            <div class="loader-inner"></div>
        </div>
    </div>

    <!-- HEADER -->
    <header>
        <img id="CoderOmLogo" src="~/Content/projectAssets/Images/About/@ViewBag.About.Logo" alt="">


        <div class="nav-icon">
            <span></span>
            <span></span>
            <span></span>
        </div>
    </header>

    <!-- FULL MENU -->
    <div class="full-menu">
        <div class="full-inner row">
            <nav class="col-md-8">
                <ul>
                    <li><a href="~/">Home</a></li>
                    <li><a href="@Url.RouteUrl("About")">About me</a></li>
                    <li><a href="@Url.RouteUrl("Projects")">Work</a></li>
                    <li><a href="@Url.RouteUrl("Contact")">Contact</a></li>
                    <li>
                        <a href="@Url.Action("DownloadResume", "Home")">Get My Resume</a>
                    </li>
                </ul>
            </nav>
            <div class="col-md-4 full-contact">
                <ul>
                    <li class="title">Get in Touch</li>
                    <li>mail.coderom@gmail.com</li>
                    <li>
                        <div class="social">
                            <a href="@ViewBag.About.Linkedin"><i class="fab fa-linkedin"></i> </a>
                            <a href="@ViewBag.About.Github"><i class="fab fa-github" aria-hidden="true"></i> </a>
                            <a href="@ViewBag.About.Twitter"><i class="fab fa-twitter" aria-hidden="true"></i> </a>
                            <a href="@ViewBag.About.Instagram"><i class="fab fa-instagram" aria-hidden="true"></i> </a>
                            <a href="@ViewBag.About.Youtube"><i class="fab fa-youtube" aria-hidden="true"></i> </a>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    @RenderBody()

    <footer>
        <div class="cont">
            <div class="row">
                <div class="col-md-4 col-sm-6 col-xs-12 copyright">
                    <img src="~/Content/projectAssets/Images/About/@ViewBag.About.Logo" alt="">
                    <p>© @DateTime.Now.Year Coder Om</p>
                </div>
                <div class="col-md-4 d-sm-none d-md-block">
                    <div class="social">
                        <a href="@ViewBag.About.Linkedin"><i class="fab fa-linkedin"></i> </a>
                        <a href="@ViewBag.About.Github"><i class="fab fa-github" aria-hidden="true"></i> </a>
                        <a href="@ViewBag.About.Twitter"><i class="fab fa-twitter" aria-hidden="true"></i> </a>
                        <a href="@ViewBag.About.Instagram"><i class="fab fa-instagram" aria-hidden="true"></i> </a>
                        <a href="@ViewBag.About.Youtube"><i class="fab fa-youtube" aria-hidden="true"></i> </a>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12 d-none d-sm-block getintouch">
                    <a href="mailto:@ViewBag.About.Email">
                        <strong>Get In Touch</strong><br>
                        <p>mail.coderom@gmail.com</p>
                    </a>
                </div>
            </div>
        </div>
    </footer>

    <!-- Javascripts -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/gsap.min.js" integrity="sha512-16esztaSRplJROstbIIdwX3N97V1+pZvV33ABoG1H2OyTttBxEGkTsoIVsiP1iaTtM8b3+hu2kB6pQ4Clr5yug==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="~/Content/js/jquery-2.1.4.min.js"></script>
    <script src="~/Content/cubeportfolio/js/jquery.cubeportfolio.min.js"></script>
    <script src="~/Content/js/typed.js"></script>
    <script src="~/Content/js/particles.js"></script>
    <script src="~/Content/js/app.js"></script>
    <script src="~/Content/js/jquery.hover3d.js"></script>
    <script src="~/Content/js/owl.carousel.min.js"></script>
    <script src="~/Content/js/main.js"></script>

</body>
</html>
